{
  "name": "eso-log-aggregator",
  "version": "0.1.0",
  "private": true,
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass",
    "*.less",
    "./src/index.tsx",
    "./src/setupTests.ts"
  ],
  "dependencies": {
    "@apollo/client": "^3.13.9",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@graphql-codegen/near-operation-file-preset": "^3.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@tanstack/react-table": "^8.21.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "chartjs-plugin-annotation": "^3.1.0",
    "graphql": "^16.11.0",
    "history": "^5.3.0",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.8.2",
    "redux-persist": "^6.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/659.*.chunk.js",
    "analyze-all": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "analyze-bundle": "npm run build && node analyze-bundle.js",
    "test": "craco test --watchAll=false",
    "test:watch": "craco test",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint",
    "lint": "eslint ./src --ext .ts,.tsx",
    "lint:fix": "eslint ./src --ext .ts,.tsx --fix",
    "format": "prettier --write ./src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "codegen": "ts-node run-codegen.ts",
    "fetch-abilities": "ts-node scripts/fetchAbilitiesToJson.ts"
  },
  "jest": {},
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ],
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typed-document-node": "^5.1.2",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/preset-create-react-app": "^9.1.3",
    "@storybook/react-webpack5": "^9.1.3",
    "@swc/core": "^1.13.5",
    "@types/axios": "^0.9.36",
    "@types/react-router-dom": "^5.3.3",
    "babel-plugin-import": "^1.13.8",
    "circular-dependency-plugin": "^5.2.2",
    "craco-alias": "^3.0.1",
    "eslint-plugin-storybook": "^9.1.3",
    "eslint": "^8.57.1",
    "ignore-loader": "^0.1.2",
    "prettier": "^3.6.2",
    "prop-types": "^15.8.1",
    "raw-loader": "^4.0.2",
    "react-scripts": "^5.0.1",
    "source-map-explorer": "^2.5.3",
    "storybook-addon-remix-react-router": "^5.0.0",
    "storybook": "^9.1.3",
    "swc-loader": "^0.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "webpack": "^5.101.3",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}