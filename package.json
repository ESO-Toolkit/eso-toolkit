  "msw": {
    "workerDirectory": [
      ".storybook/public"
    ]
  },
  "name": "eso-log-aggregator",
  "private": true,
  "scripts": {
    "analyze": "npm run build && npx vite-bundle-analyzer build/assets/*.js",
    "analyze-affix-scripts": "node scripts/analyze-affix-scripts.js",
    "analyze-affix-scripts:all": "node scripts/analyze-affix-scripts.js all",
    "analyze-all": "npm run build && npx vite-bundle-analyzer build/assets/*.js",
    "analyze-bundle": "npm run build && node scripts/analyze-bundle.cjs",
    "analyze-scribing-skills": "node scripts/analyze-scribing-skills.cjs",
    "analyze-scribing-recipes": "ts-node --project tsconfig.node.json scripts/scribing-analysis-runner.ts",
    "cache:stats": "node scripts/manage-cache-simple.js stats",
    "cache:clean": "node scripts/manage-cache-simple.js clean",
    "cache:clear": "node scripts/manage-cache-simple.js clear",
    "analyze-scribing-recipes:no-cache": "ts-node --project tsconfig.node.json scripts/scribing-analysis-runner.ts --no-optimize --clear-cache",
    "analyze-scribing-recipes:config": "ts-node --project tsconfig.node.json scripts/scribing-analysis-runner.ts --config",
    "build": "npm run prebuild && cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" vite build",
    "build-storybook": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" storybook build",
    "clean:version": "node scripts/clean-version.cjs",
    "codegen": "npm run script -- scripts/run-codegen.ts",
    "coverage:analyze": "node scripts/coverage-analyzer.cjs",
    "coverage:analyze:threshold": "node scripts/coverage-analyzer.cjs --threshold",
    "coverage:analyze:watch": "node scripts/coverage-analyzer.cjs --watch",
    "coverage:badges": "node scripts/coverage-badge-generator.cjs",
    "coverage:full": "npm run test:coverage && npm run coverage:badges && npm run coverage:analyze",
    "coverage:gist": "node scripts/upload-coverage-to-gist.cjs",
    "coverage:open": "start coverage/lcov-report/index.html",
    "dev": "node scripts/generate-version.cjs && vite",
    "discover-missing-sets": "npm run script -- scripts/discover-missing-sets.ts",
    "discover-sets-personal": "npm run script -- scripts/discover-sets-from-personal-logs.ts",
    "download-report-data": "npm run script -- scripts/download-report-data.ts",
    "download-test-data": "npm run script -- scripts/download-test-data.ts",
    "extract-grimoire-ids": "node scripts/extract-grimoire-ids.js",
    "fetch-abilities": "npm run script -- scripts/fetchAbilitiesToJson.ts",
    "fetch-scribing-combinations": "node scripts/fetch-scribing-combinations.js",
    "fetch-scribing-combinations-browser": "node scripts/fetch-scribing-combinations-browser.js",
    "fetch-scribing-data": "npm run script -- scripts/fetchScribingData.ts",
    "fix:cache-busting": "node scripts/fix-cache-busting.cjs",
    "generate:skill-icons": "tsx --tsconfig tsconfig.scripts.json scripts/generate-skill-icons.ts",
    "generate:item-sets": "npm run script -- scripts/generate-item-set-collections.ts",
    "script": "tsx --tsconfig tsconfig.scripts.json",
    "skill-lines:generate:class": "npm run script -- scripts/generate-class-skill-configs.ts",
    "format": "prettier --write ./src/**/*.{ts,tsx,js,jsx,json,css,md,graphql}",
    "format:check": "prettier --check ./src/**/*.{ts,tsx,js,jsx,json,css,md,graphql}",
    "generate-version": "node scripts/generate-version.cjs",
    "health-check": "node scripts/health-check.cjs",
    "deploy": "node scripts/deploy.cjs deploy",
    "deploy:validate": "node scripts/deploy.cjs validate",
    "deploy:health": "node scripts/deploy.cjs health-check",
    "docker:build": "docker build -t eso-log-aggregator .",
    "docker:run": "docker run -p 80:80 eso-log-aggregator",
    "docker:compose": "docker-compose up -d",
    "docker:compose:build": "docker-compose up -d --build",
    "docker:compose:down": "docker-compose down",
    "shrinkwrap:create": "node scripts/scribing-shrinkwrap.js create",
    "shrinkwrap:validate": "node scripts/scribing-shrinkwrap.js validate",
    "shrinkwrap:update": "node scripts/scribing-shrinkwrap.js update",
    "lint": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" eslint ./src --max-warnings 0",
    "lint:fix": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" eslint ./src --fix",
    "postbuild": "echo Build completed with cache-busting enabled",
    "prebuild": "node scripts/generate-version.cjs && node scripts/update-manifest.cjs",
    "preview": "vite preview",
    "queue:add": "node scripts/merge-queue/manage-queue.cjs add",
    "queue:help": "node scripts/merge-queue/manage-queue.cjs help",
    "queue:process": "node scripts/merge-queue/manage-queue.cjs process",
    "queue:remove": "node scripts/merge-queue/manage-queue.cjs remove",
    "queue:status": "node scripts/merge-queue/manage-queue.cjs status",
    "start": "node scripts/generate-version.cjs && vite",
    "start:dev": "node scripts/generate-version.cjs && vite --mode development",
    "storybook": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" storybook dev -p 6006",
    "sync-jira": "npm run script -- scripts/sync-jira-status.ts",
    "sync-jira:apply": "npm run script -- scripts/sync-jira-status.ts --apply",
    "sync-jira:verbose": "npm run script -- scripts/sync-jira-status.ts --verbose",
    "test": "jest --onlyChanged --passWithNoTests",
    "test:performance:script": "ts-node --project tsconfig.node.json scripts/performance-test.ts",
    "test:all": "jest",
    "test:changed": "jest --onlyChanged",
    "test:changed-since": "jest --changedSince=main",
    "test:integration": "jest --config=jest.integration.config.cjs --watchAll=false",
    "test:integration:watch": "jest --config=jest.integration.config.cjs --watch",
    "test:integration:coverage": "jest --config=jest.integration.config.cjs --coverage --watchAll=false",
    "test:coverage": "cross-env COVERAGE=true jest --coverage --watchAll=false",
    "test:coverage:ci": "cross-env NODE_ENV=ci COVERAGE_MODE=ci jest --config=jest.ci.config.cjs",
    "test:coverage:report": "npm run test:coverage && npm run coverage:open",
    "test:coverage:watch": "set COVERAGE=true && jest --coverage --watchAll",
    "test:scribing": "jest --config=jest.scribing.config.cjs",
    "test:last-commit": "jest --lastCommit",
    "test:nightly:all": "playwright test --config=playwright.nightly.config.ts",
    "test:nightly:auth": "playwright test --config=playwright.nightly.config.ts --project=chromium-desktop-auth",
    "test:nightly:chromium": "playwright test --config=playwright.nightly.config.ts --project=chromium-desktop",
    "test:nightly:debug": "playwright test --config=playwright.nightly.config.ts --headed --project=chromium-desktop-auth --max-failures=1",
    "test:nightly:firefox": "playwright test --config=playwright.nightly.config.ts --project=firefox-desktop",
    "test:nightly:headed": "playwright test --config=playwright.nightly.config.ts --headed --project=chromium-desktop",
    "test:nightly:mobile": "playwright test --config=playwright.nightly.config.ts --project=mobile-chrome --project=mobile-safari",
    "test:nightly:report": "playwright show-report playwright-report-nightly",
    "test:nightly:shard1": "cross-env SHARD_INDEX=1 SHARD_TOTAL=3 playwright test --config=playwright.nightly.config.ts",
    "test:nightly:shard2": "cross-env SHARD_INDEX=2 SHARD_TOTAL=3 playwright test --config=playwright.nightly.config.ts",
    "test:nightly:shard3": "cross-env SHARD_INDEX=3 SHARD_TOTAL=3 playwright test --config=playwright.nightly.config.ts",
    "test:nightly:sharded": "npm-run-all --parallel test:nightly:shard1 test:nightly:shard2 test:nightly:shard3",
    "test:nightly:sharded:chromium": "npm-run-all --parallel \"test:nightly:shard* -- --project=chromium-desktop\"",
    "test:nightly:sharded:firefox": "npm-run-all --parallel \"test:nightly:shard* -- --project=firefox-desktop\"",
    "test:screen-sizes": "playwright test --config=playwright.screen-sizes.config.ts",
    "test:screen-sizes:setup": "npm run download-test-data",
    "test:screen-sizes:fast": "playwright test --config=playwright.screen-sizes-fast.config.ts",
    "test:screen-sizes:headed": "playwright test --config=playwright.screen-sizes.config.ts --headed",
    "test:screen-sizes:ui": "playwright test --config=playwright.screen-sizes.config.ts --ui",
    "test:screen-sizes:mobile": "playwright test --config=playwright.screen-sizes.config.ts --project=\"Mobile*\" --project=\"Android*\"",
    "test:screen-sizes:tablet": "playwright test --config=playwright.screen-sizes.config.ts --project=\"Tablet*\"",
    "test:screen-sizes:desktop": "playwright test --config=playwright.screen-sizes.config.ts --project=\"Desktop*\"",
    "test:screen-sizes:breakpoints": "playwright test --config=playwright.screen-sizes.config.ts --project=\"Breakpoint*\"",
    "test:screen-sizes:report": "playwright show-report screen-size-report",
    "test:screen-sizes:update-snapshots": "playwright test --config=playwright.screen-sizes.config.ts --update-snapshots",
    "test:screen-sizes:debug": "playwright test --config=playwright.screen-sizes.config.ts --headed --project=\"Desktop Standard\" --max-failures=1 --debug",
    "verify:screen-size-setup": "node scripts/verify-screen-size-setup.cjs",
    "test:nightly:sharded:win": "start \"Shard 1\" cmd /c \"npm run test:nightly:shard1\" && start \"Shard 2\" cmd /c \"npm run test:nightly:shard2\" && start \"Shard 3\" cmd /c \"npm run test:nightly:shard3\"",
    "test:nightly:webkit": "playwright test --config=playwright.nightly.config.ts --project=webkit-desktop",
    "test:smoke": "npm run test:smoke:unit && npm run test:smoke:e2e",
    "test:smoke:e2e": "playwright test --config=playwright.smoke.config.ts",
    "test:smoke:unit": "jest --config=jest.smoke.config.cjs --watchAll=false",
    "test:full": "playwright test --config=playwright.full.config.ts",
    "test:full:headed": "playwright test --config=playwright.full.config.ts --headed",
    "test:full:ui": "playwright test --config=playwright.full.config.ts --ui",
    "test:full:report": "playwright show-report playwright-report-full",
    "test:performance": "playwright test --config=playwright.performance.config.ts",
    "test:performance:headed": "playwright test --config=playwright.performance.config.ts --headed",
    "test:performance:report": "playwright show-report playwright-report-performance",
    "test:watch": "jest --watch",
    "test:watch-changed": "jest --watch --onlyChanged",
    "test:debug": "playwright test --config=playwright.debug.config.ts",
    "test:debug:headed": "playwright test --config=playwright.debug.config.ts --headed",
    "test:debug:ui": "playwright test --config=playwright.debug.config.ts --ui",
    "type-check": "npm run typecheck",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run format:check",
    "validate:playwright-ai": "node scripts/validate-playwright-ai.cjs",
    "validate:preloading": "node scripts/validate-playwright-ai.cjs tests/visual-*.spec.ts"
  },
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass",
    "*.less",
    "./src/index.tsx",
    "./src/setupTests.ts"
  ],
  "version": "0.1.0"
}
