{
  "name": "eso-log-aggregator",
  "version": "0.1.0",
  "private": true,
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass",
    "*.less",
    "./src/index.tsx",
    "./src/setupTests.ts"
  ],
  "dependencies": {
    "@apollo/client": "^3.13.9",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@graphql-codegen/near-operation-file-preset": "^3.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@sentry/react": "^10.7.0",
    "@sentry/tracing": "^7.120.4",
    "@tanstack/react-table": "^8.21.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "chartjs-plugin-annotation": "^3.1.0",
    "comlink": "^4.4.2",
    "date-fns": "^4.1.0",
    "graphql": "^16.11.0",
    "history": "^5.3.0",
    "memoize-one": "^6.0.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.8.2",
    "react-sticky": "^6.0.3",
    "react-sticky-box": "^2.0.5",
    "react-text-gradients": "^1.0.2",
    "redux-first-history": "^5.2.0",
    "redux-persist": "^6.0.0",
    "storybook-addon-remix-react-router": "^5.0.0",
    "three": "^0.169.0",
    "uuid": "^11.1.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "analyze-all": "npm run build && npx vite-bundle-analyzer build/assets/*.js",
    "analyze-bundle": "npm run build && node analyze-bundle.js",
    "analyze-scribing-skills": "node scripts/analyze-scribing-skills.js",
    "analyze": "npm run build && npx vite-bundle-analyzer build/assets/*.js",
    "benchmark": "node benchmark-dev-server.js",
    "build-storybook": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" storybook build",
    "build": "npm run prebuild && cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" vite build",
    "prebuild": "node scripts/generate-version.js && node scripts/update-manifest.js",
    "postbuild": "echo Build completed with cache-busting enabled",
    "clean:version": "node scripts/clean-version.js",
    "codegen": "node --loader ts-node/esm run-codegen.mts",
    "coverage:analyze:threshold": "node scripts/coverage-analyzer.js --threshold",
    "coverage:analyze:watch": "node scripts/coverage-analyzer.js --watch",
    "coverage:analyze": "node scripts/coverage-analyzer.js",
    "coverage:badges": "node scripts/coverage-badge-generator.js",
    "coverage:full": "npm run test:coverage && npm run coverage:badges && npm run coverage:analyze",
    "coverage:open": "start coverage/lcov-report/index.html",
    "dev": "vite",
    "download-report-data": "npx tsx scripts/download-report-data.ts",
    "fetch-abilities": "ts-node scripts/fetchAbilitiesToJson.ts",
    "format:check": "prettier --check ./src/**/*.{ts,tsx,js,jsx,json,css,md,graphql}",
    "format": "prettier --write ./src/**/*.{ts,tsx,js,jsx,json,css,md,graphql}",
    "lint:fix": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" eslint ./src --ext .ts,.tsx --fix",
    "lint": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" eslint ./src --ext .ts,.tsx",
    "preview": "vite preview",
    "start:dev": "vite --mode development",
    "start": "vite",
    "storybook": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" storybook dev -p 6006",
    "test-scribing-skills": "npx tsx scripts/scribing-skills-test.ts",
    "test:coverage:ci": "set NODE_ENV=ci && set COVERAGE_MODE=ci && set COVERAGE=true && jest --coverage --watchAll=false",
    "test:coverage:report": "npm run test:coverage && npm run coverage:open",
    "test:coverage:watch": "set COVERAGE=true && jest --coverage --watchAll",
    "test:coverage": "set COVERAGE=true && jest --coverage --watchAll=false",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e": "playwright test --project=chromium",
    "test:watch": "jest",
    "test": "jest",
    "type-check": "npm run typecheck",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run format:check"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typed-document-node": "^5.1.2",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.1",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-links": "^9.1.3",
    "@storybook/react-vite": "^9.1.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.11",
    "@types/three": "^0.169.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-storybook": "^9.1.5",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "^30.1.1",
    "jest-environment-jsdom": "^30.1.1",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^3.0.1",
    "msw": "^2.10.5",
    "msw-storybook-addon": "^2.0.5",
    "prettier": "^3.6.2",
    "prop-types": "^15.8.1",
    "source-map-explorer": "^2.5.3",
    "storybook": "^9.1.3",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^6.3.5",
    "vite-bundle-analyzer": "^1.2.2",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "webpack": "^5.101.3",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "msw": {
    "workerDirectory": [
      ".storybook/public",
      ".storybook\\public"
    ]
  }
}
