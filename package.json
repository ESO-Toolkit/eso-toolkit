{
  "name": "eso-log-aggregator",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass",
    "*.less",
    "./src/index.tsx",
    "./src/setupTests.ts"
  ],
  "dependencies": {
    "@apollo/client": "^3.13.9",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@graphql-codegen/near-operation-file-preset": "^3.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.0.0",
    "@reduxjs/toolkit": "^2.9.0",
    "@sentry/node": "^10.12.0",
    "@sentry/react": "^10.12.0",
    "@sentry/tracing": "^7.120.4",
    "@simonwep/pickr": "^1.9.1",
    "@tanstack/react-table": "^8.21.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.0.0",
    "@types/react-window": "^2.0.0",
    "ajv": "^8.17.1",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "chartjs-plugin-annotation": "^3.1.0",
    "comlink": "^4.4.2",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.19",
    "graphql": "^16.11.0",
    "history": "^5.3.0",
    "memoize-one": "^6.0.0",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.3",
    "react-sticky": "^6.0.3",
    "react-sticky-box": "^2.0.5",
    "react-text-gradients": "^1.0.2",
    "react-window": "^2.1.2",
    "react-window-infinite-loader": "^1.0.10",
    "redux-first-history": "^5.2.0",
    "redux-persist": "^6.0.0",
    "three": "^0.180.0",
    "uuid": "^11.1.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "analyze-all": "npm run build && npx vite-bundle-analyzer build/assets/*.js",
    "analyze-bundle": "npm run build && node scripts/analyze-bundle.cjs",
    "analyze-scribing-skills": "node scripts/analyze-scribing-skills.cjs",
    "analyze": "npm run build && npx vite-bundle-analyzer build/assets/*.js",
    "benchmark": "node benchmark-dev-server.js",
    "build-storybook": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" storybook build",
    "build": "npm run prebuild && cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" vite build",
    "prebuild": "node scripts/generate-version.cjs && node scripts/update-manifest.cjs",
    "postbuild": "echo Build completed with cache-busting enabled",
    "clean:version": "node scripts/clean-version.cjs",
    "codegen": "node --loader ts-node/esm scripts/run-codegen.mts",
    "coverage:analyze:threshold": "node scripts/coverage-analyzer.cjs --threshold",
    "coverage:analyze:watch": "node scripts/coverage-analyzer.cjs --watch",
    "coverage:analyze": "node scripts/coverage-analyzer.cjs",
    "coverage:badges": "node scripts/coverage-badge-generator.cjs",
    "coverage:full": "npm run test:coverage && npm run coverage:badges && npm run coverage:analyze",
    "coverage:gist": "node scripts/upload-coverage-to-gist.cjs",
    "coverage:open": "start coverage/lcov-report/index.html",
    "dev": "node scripts/generate-version.cjs && vite",
    "download-report-data": "npx tsx scripts/download-report-data.ts",
    "fetch-abilities": "ts-node scripts/fetchAbilitiesToJson.ts",
    "fix:cache-busting": "node scripts/fix-cache-busting.cjs",
    "format:check": "prettier --check ./src/**/*.{ts,tsx,js,jsx,json,css,md,graphql}",
    "format": "prettier --write ./src/**/*.{ts,tsx,js,jsx,json,css,md,graphql}",
    "health-check": "node scripts/health-check.cjs",
    "lint:fix": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" eslint ./src --fix",
    "lint": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" eslint ./src",
    "preview": "vite preview",
    "start:dev": "node scripts/generate-version.cjs && vite --mode development",
    "start": "node scripts/generate-version.cjs && vite",
    "storybook": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" storybook dev -p 6006",
    "test-scribing-skills": "npx tsx scripts/scribing-skills-test.ts",
    "test:all": "jest",
    "test:changed-since": "jest --changedSince=main",
    "test:changed": "jest --onlyChanged",
    "test:coverage:ci": "set NODE_ENV=ci && set COVERAGE_MODE=ci && set COVERAGE=true && jest --coverage --watchAll=false",
    "test:coverage:report": "npm run test:coverage && npm run coverage:open",
    "test:coverage:watch": "set COVERAGE=true && jest --coverage --watchAll",
    "test:coverage": "set COVERAGE=true && jest --coverage --watchAll=false",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e": "playwright test --project=chromium",
    "test:last-commit": "jest --lastCommit",
    "test:nightly:all": "playwright test --config=playwright.nightly.config.ts",
    "test:nightly:auth": "playwright test --config=playwright.nightly.config.ts --project=chromium-desktop-auth",
    "test:nightly:chromium": "playwright test --config=playwright.nightly.config.ts --project=chromium-desktop",
    "test:nightly:debug": "playwright test --config=playwright.nightly.config.ts --headed --project=chromium-desktop-auth --max-failures=1",
    "test:nightly:firefox": "playwright test --config=playwright.nightly.config.ts --project=firefox-desktop",
    "test:nightly:headed": "playwright test --config=playwright.nightly.config.ts --headed --project=chromium-desktop",
    "test:nightly:mobile": "playwright test --config=playwright.nightly.config.ts --project=mobile-chrome --project=mobile-safari",
    "test:nightly:report": "playwright show-report playwright-report-nightly",
    "test:nightly:shard1": "cross-env SHARD_INDEX=1 SHARD_TOTAL=3 playwright test --config=playwright.nightly.config.ts",
    "test:nightly:shard2": "cross-env SHARD_INDEX=2 SHARD_TOTAL=3 playwright test --config=playwright.nightly.config.ts",
    "test:nightly:shard3": "cross-env SHARD_INDEX=3 SHARD_TOTAL=3 playwright test --config=playwright.nightly.config.ts",
    "test:nightly:sharded": "npm-run-all --parallel test:nightly:shard1 test:nightly:shard2 test:nightly:shard3",
    "test:nightly:sharded:advanced": "node scripts/run-sharded-tests.cjs",
    "test:nightly:sharded:chromium": "npm-run-all --parallel \"test:nightly:shard* -- --project=chromium-desktop\"",
    "test:nightly:sharded:firefox": "npm-run-all --parallel \"test:nightly:shard* -- --project=firefox-desktop\"",
    "test:nightly:sharded:win": "start \"Shard 1\" cmd /c \"npm run test:nightly:shard1\" && start \"Shard 2\" cmd /c \"npm run test:nightly:shard2\" && start \"Shard 3\" cmd /c \"npm run test:nightly:shard3\"",
    "test:nightly:webkit": "playwright test --config=playwright.nightly.config.ts --project=webkit-desktop",
    "test:smoke:e2e": "playwright test --config=playwright.smoke.config.ts",
    "test:smoke:unit": "jest --config=jest.smoke.config.cjs --watchAll=false",
    "test:smoke": "npm run test:smoke:unit && npm run test:smoke:e2e",
    "test:watch-changed": "jest --watch --onlyChanged",
    "test:watch": "jest --watch",
    "test": "jest --onlyChanged --passWithNoTests",
    "type-check": "npm run typecheck",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint && npm run format:check"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typed-document-node": "^5.1.2",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.1",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-docs": "^9.1.7",
    "@storybook/addon-links": "^9.1.7",
    "@storybook/react-vite": "^9.1.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.17",
    "@types/react-color": "^3.0.13",
    "@types/three": "^0.180.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "cross-env": "^7.0.3",
    "cross-fetch": "^4.1.0",
    "dotenv": "^16.6.1",
    "eslint": "^9.36.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^3.0.1",
    "msw": "^2.11.3",
    "msw-storybook-addon": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "prop-types": "^15.8.1",
    "source-map-explorer": "^2.5.3",
    "storybook": "^9.1.3",
    "terser": "^5.44.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^6.3.6",
    "vite-bundle-analyzer": "^1.2.2",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "msw": {
    "workerDirectory": [
      ".storybook/public"
    ]
  }
}
