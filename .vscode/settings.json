{
    "python-envs.defaultEnvManager": "ms-python.python:system",
    "python-envs.pythonProjects": [],
    "github.copilot.chat.mcp.enabled": true,
    "github.copilot.chat.mcp.servers": {
        "eso-log-aggregator-testing": {
            "command": "node",
            "args": [
                "${workspaceFolder}\\.github\\copilot-skills\\testing\\server.js"
            ],
            "env": {
                "AUTH_STATE_PATH": "${workspaceFolder}\\tests\\auth-state.json",
                "BASE_URL": "http://localhost:3000",
                "PLAYWRIGHT_CONFIG": "${workspaceFolder}\\playwright.debug.config.ts"
            }
        },
        "eso-log-aggregator-jira": {
            "command": "node",
            "args": [
                "${workspaceFolder}\\.github\\copilot-skills\\jira\\server.js"
            ],
            "env": {
                "DEBUG": "false"
            }
        },
        "eso-log-aggregator-reports": {
            "command": "node",
            "args": [
                "${workspaceFolder}\\.github\\copilot-skills\\reports\\server.js"
            ],
            "env": {
                "DEBUG": "false"
            }
        },
        "eso-log-aggregator-git": {
            "command": "node",
            "args": [
                "${workspaceFolder}\\.github\\copilot-skills\\git\\server.js"
            ],
            "env": {
                "DEBUG": "false"
            }
        },
        "eso-log-aggregator-rebase": {
            "command": "node",
            "args": [
                "${workspaceFolder}\\.github\\copilot-skills\\rebase\\index.js"
            ],
            "env": {
                "DEBUG": "false"
            }
        },
        "eso-log-aggregator-uesp-data": {
            "command": "node",
            "args": [
                "${workspaceFolder}\\.copilot\\uesp-data\\server.js"
            ],
            "env": {
                "DEBUG": "false"
            }
        }
    },
    "terminal.integrated.profiles.windows": {
        "PowerShell (.env)": {
            "source": "PowerShell",
            "args": [
                "-NoExit",
                "-Command",
                "if (Test-Path .env) { Get-Content .env | ForEach-Object { if ($_ -match '^([^#=][^=]*)=(.*)$') { [System.Environment]::SetEnvironmentVariable($Matches[1].Trim(), $Matches[2].Trim(), 'Process') } } }"
            ]
        }
    },
    "terminal.integrated.defaultProfile.windows": "PowerShell (.env)"
}